import java.util.*;

public class NineInts {
    public static void main(String[] args) {
        int[] numbers = {10, 15, 19, 23, 26, 29, 31, 34, 38};

        for (int x = 0; x < 9; x++) {
            System.out.print(numbers[x] + " ");
        }
        System.out.println();
        for (int x = 8; x >= 0; x--) {
            System.out.print(numbers[x] + " ");
        }
    }
}
